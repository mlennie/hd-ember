#top-search-bar
	if session.isAuthenticated
		unless session.currentUser.isOwner
			div.hidden-sm.hidden-xs
				nav.navbar.grey-background#top-search-medium
					.row
						.col-lg-12.col-md-12
							form submit='search'
								span.input_container
									= bootstrap-datepicker value=date class="input" id="date-field" readonly="true" format="dd/mm/yy" todayBtn=false todayHighlight=true placeholder="Date" language='fr'
									span.search-icon = fa-icon 'calendar'
								span.input_container
									= view Ember.Select content=hours value=hour class="search-select input" prompt="Heure"
									span.search-icon = fa-icon 'caret-down'
								span.input_container
									= view Ember.Select content=couverts value=nbCouverts class="search-select input"
									span.search-icon = fa-icon 'caret-down'
								span.input_container
									= view Ember.Select content=cuisines value=cuisine class="search-select input" prompt="Cuisine" disabled=isDisabled
									span.search-icon = fa-icon 'caret-down'
								span.input_container
									= view Ember.Select content=names prompt="OÃ¹ ?" value=name id="name-select" class="search-select input"
									span.search-icon = fa-icon 'caret-down'
								button#top-search-button type="submit" Rechercher